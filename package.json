    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.10.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "cypress": "^14.3.3",
    "eslint": "^9.26.0",
    "jest": "^29.7.0",
    "mocha": "^11.2.2",
    "owasp-dependency-check": "^0.0.25",
    "webpack": "^5.99.8",
    "webpack-cli": "^6.0.1"
  }
