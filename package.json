  "description": "A web app linking passengers and drivers travelling long distances in Kenya.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "build": "webpack --config webpack.prod.js",
